<%
	# compact version of available event log actions based on activity type
	# for example used in dashboard.html.erb
%>
<div class="action clearfix">
						<ul>
							<li><%= time_ago_in_words event_log.created_at %> ago</li>
							<% if event_log.event_type.new_item_request_requester? || event_log.event_type.new_item_request_gifter? %>
	              <li>
		              <%= link_to "view request", request_path(event_log.related) %>
	              </li>
              <% end %>
							<li><a href="#">offer</a></li>
							<li><a href="#">comments(0)</a></li>
						</ul>
</div>
